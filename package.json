{
  "name": "lines-juggler",
  "version": "0.1.0",
  "description": "Lines manipulation library",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "./node_modules/lab/bin/lab -a code -v -t 100",
    "lint": "./node_modules/.bin/prettier --write '**/*.js'",
    "ts-doc": "./node_modules/.bin/jsdoc -t node_modules/@otris/jsdoc-tsd -r ./lib -d ./lib/index.d.ts",
    "md-doc": "./node_modules/jsdoc-to-markdown/bin/cli.js lib/index.js > ./API.md"
  },
  "opts": {
    "template": "./node_modules/@otris/jsdoc-tsd"
  },
  "keywords": [
    "lines",
    "string",
    "manipulation"
  ],
  "author": "Edouard Demotes <edouard@tractr.net>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/Tractr/lines-juggler/issues"
  },
  "homepage": "https://github.com/Tractr/lines-juggler#readme",
  "devDependencies": {
    "@otris/jsdoc-tsd": "^1.0.4",
    "code": "^5.2.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "lab": "^15.3.0",
    "prettier": "^1.14.3"
  }
}
